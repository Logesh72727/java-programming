import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int n;
        try{
        n = scan.nextInt();
        }catch(Exception e){
            System.out.println("invalid input");
            return;
        }
        if(n>0){
            System.out.println("positive");
        }else if(n<0){
            System.out.println("negative");
        }else{
            System.out.println("zero");
        }
    }
}
