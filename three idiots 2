import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        float a = scan.nextInt(), b = scan.nextInt(), c = scan.nextInt(), d= scan.nextInt();
        System.out.printf("Binoy's house is located at (%.1f,%.1f)",(a+c)/2,(b+d)/2);
    }
}
