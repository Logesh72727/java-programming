import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int n = scan.nextInt(),n1;
        n1=n;
        int result = 0;
        while(n1>0){
            result+=n1%10;
            n1/=10;
        }
        if(n%result==0)
            System.out.println("Harshad Number");
        else
            System.out.println("Not Harshad Number");
    }
}
