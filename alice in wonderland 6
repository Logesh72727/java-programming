import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int n = scan.nextInt();
        System.out.println("Bird said:"+n);
        int sum = 0;
        while(n>0){
            sum+=n%10;
            n/=10;
        }
        System.out.println("Alice must go in path-"+sum);
        
    }
}
