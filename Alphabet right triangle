import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int n = scan.nextInt();
        char temp,ch = 'A';
        for(int i=0; i<n; i++){
            temp = ch;
            for(int j=0; j<=i; j++){
                System.out.print(ch+++" ");
            }System.out.println();
            ch=++temp;
        }
    }
}
