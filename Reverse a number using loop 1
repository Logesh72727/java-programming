import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int n = scan.nextInt(),result=0;
        if(n>0){
            while(n>0){
                result*=10;
                result+=n%10;
                n/=10;
            }System.out.println(result);
        }else{
            System.out.println("Invalid Input");
        }
    }
}
